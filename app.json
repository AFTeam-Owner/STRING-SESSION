{
  "name": "string session maker bot",
  "description": "A Telegram Bot to generate Telethon and Pyrogram String Sessions.",
  "logo": "https://telegra.ph/file/9f31b3a61c4897b297a33.jpg",
  "keywords": [
    "telegram",
    "bot",
    "python",
    "pyrogram"
  ],
    "env": {
        "ENVIRONMENT": {
      "description": "Don't change ANYTHING.",
      "required": true,
      "value": "ANYTHING"
    },
    "API_ID": {
      "description": "Get this value from my.telegram.org/apps.",
      "required": true,
      "value": ""
    },
    "API_HASH": {
      "description": "Get this value from my.telegram.org/apps.",
      "required": true,
      "value": ""
    },
    "OWNER_ID": {
      "description": "Telegram id of the person who will known as owner of the bot.",
      "required": true,
      "value": ""
    },
    "MONGO_DB_URI" {
      "description": "Get this value from cloud.mongodb.com",
      "required": true,
      "value": ""
    },
    "MUST_JOIN": {
      "description": "Username of a group or channel for which you want to set force sub.(without @)",
      "required": true,
      "value": "Best_FriendsFor_Ever"
    },
    "BOT_TOKEN": {
      "description": "Get this value from @BotFather",
      "required": true,
      "value": ""
      }
    },
    "addons": [
        {
            "plan": "heroku-postgresql"
        }
    ]
}
